		<Rule name="Kawasan Suaka Alam/Kawasan Pelestarian Alam">
           	<Filter>( !([fungsi] = 'HL' || [fungsi] = 'HPT' || [fungsi] = 'HP' || [fungsi] = 'HPK' || [fungsi] = 'APL'  ) )</Filter>
            <!--<Filter>(!([fungsi]='HL' || [fungsi]='HPT'))</Filter>-->
            <LineSymbolizer stroke="rgb(169,67,255)" stroke-width="0.26" stroke-linejoin="bevel"/>
            <PolygonSymbolizer fill="rgb(169,67,255)"/>            
            <!--<MaxScaleDenominator>1000000</MaxScaleDenominator>-->
        </Rule>        
        <Rule name="Hutan Lindung">
            <Filter>([fungsi]='HL')</Filter>
            <LineSymbolizer stroke="rgb(94,255,67)" stroke-width="0.26" stroke-linejoin="bevel"/>
            <PolygonSymbolizer fill="rgb(94,255,67)"/>            
            <!--<MaxScaleDenominator>1000000</MaxScaleDenominator>-->
        </Rule>
        <Rule name="Hutan Produksi Terbatas">
            <Filter>([fungsi]='HPT')</Filter>
            <LineSymbolizer stroke="rgb(191,255,108)" stroke-width="0.26" stroke-linejoin="bevel"/>
