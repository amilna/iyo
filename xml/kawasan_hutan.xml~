            <!--<MaxScaleDenominator>1000000</MaxScaleDenominator>-->
        </Rule>
        <Rule name="Hutan Produksi Tetap">
            <Filter>([fungsi]='HP')</Filter>
            <LineSymbolizer stroke="rgb(255,255,110)" stroke-width="0.26" stroke-linejoin="bevel"/>
            <PolygonSymbolizer fill="rgb(255,255,110)"/>            
            <!--<MaxScaleDenominator>1000000</MaxScaleDenominator>-->
        </Rule>
